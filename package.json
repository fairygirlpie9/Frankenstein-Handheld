{
  "name": "frankenstein-ritual-game",
  "version": "1.0.0",
  "type": "module",
  "description": "Browser-based interactive Frankenstein surgical game with three.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:unit": "vitest --run --testNamePattern=\"^((?!Property).)*$\"",
    "test:pbt": "vitest --run --testNamePattern=\"Property\""
  },
  "dependencies": {
    "@tweenjs/tween.js": "^23.1.1",
    "three": "^0.160.0",
    "tone": "^15.1.22"
  },
  "devDependencies": {
    "@types/three": "^0.160.0",
    "fast-check": "^3.15.0",
    "jsdom": "^27.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  }
}
